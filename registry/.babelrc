{
  "presets": ["@babel/preset-env", ["@babel/preset-react", { "runtime": "automatic" }], "@babel/preset-typescript"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "~components": "./src/components",
          "~styles": "./src/styles"
        }
      }
    ]
  ],
  "highlightCode": false,
  "comments": false,
  "compact": true,
  "minified": true
}
